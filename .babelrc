{
    "ignore": [
      "**/*.min.js"
    ],
    "presets": [
      [
        "env",
        {
          "modules": false,
          "loose": true,
          "useBuiltIns": false,
          "targets": {
            "browsers": [
              "last 4 versions",
              "ie >= 9",
            ]
          }
        }
      ],
      "stage-2",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-runtime"
    ]
  }